{
  // 是否启用配置中心，默认启用
  "ConfigCenterEnabled": false,
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Http",
      "HealthCheck": {
        "Url": "http://localhost:3231/health",
        //单位：秒
        "Timeout": 10,
        //单位：秒
        "Interval": 10
      },
      "Name": "Wing.Demo_3.2.3",
      "Host": "localhost",
      "Port": 3231,
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "LeastConnection"
      },
      "Scheme": "http",
      "Developer": "linguicheng"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10,
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "Gateway": {
    "Policy": {
      // 全局策略
      "Global": {
        // 超时策略
        "TimeOut": {
          //是否启用
          "IsEnabled": true,
          //执行超过多少毫秒则认为超时
          "TimeOutMilliseconds": 2000
        },
        //重试
        "Retry": {
          //是否启用
          "IsEnabled": true,
          //最多重试几次
          "MaxTimes": 3,
          //重试间隔的毫秒数
          "IntervalMilliseconds": 100
        }
      }
    },
    // 请求日志
    "Log": {
      "IsEnabled": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
