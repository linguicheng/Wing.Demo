{
  // 是否启用配置中心，默认启用
  "ConfigCenterEnabled": false,
  "Consul": {
    "Url": "http://localhost:8500",
    "Service": {
      //Http  Grpc
      "Option": "Http",
      "HealthCheck": {
        "Url": "http://localhost:1240/health",
        //单位：秒
        "Timeout": 10,
        //单位：秒
        "Interval": 10
      },
      "Name": "Wing.Demo_1.2.4",
      "Host": "localhost",
      "Port": 1240,
      "LoadBalancer": {
        //RoundRobin  WeightRoundRobin LeastConnection
        "Option": "WeightRoundRobin",
        //权重
        "Weight": 50
      },
      "Scheme": "http",
      "Developer": "linguicheng"
    },
    //定时同步数据时间间隔，单位：秒 小于等于0表示立即响应
    "Interval": 10,
    //数据中心
    "DataCenter": "dc1",
    //等待时间,单位：分钟
    "WaitTime": 3
  },
  "ConnectionStrings": {
    "Wing": "Data Source=192.168.56.96;User Id=sa;Password=wing123.;Initial Catalog=Wing;TrustServerCertificate=true;Pooling=true;Min Pool Size=1"
  },
  //自动同步实体结构到数据库
  "UseAutoSyncStructure": true,
  "RabbitMQ": {
    "HostName": "192.168.56.99",
    "UserName": "admin",
    "Password": "admin",
    "VirtualHost": "/",
    "Port": 5672,
    //消息过期时间，单位：毫秒，过期会自动路由到死信队列，小于或等于0则永久有效
    "MessageTTL": 0,
    "ExchangeName": "Sample.GateWay",
    //每次投递消息数量
    "PrefetchCount": 1
  },
  "Gateway": {
    "Policy": {
      // 个性化策略
      "Policies": [
        {
          "ServiceName": "service1",
          //是否启用熔断
          "IsEnableBreaker": true,
          //熔断前出现允许错误几次
          "ExceptionsAllowedBeforeBreaking": 3,
          //熔断多长时间（毫秒）
          "MillisecondsOfBreak": 10000,
          //最多重试几次，如果为0则不重试
          "MaxRetryTimes": 0,
          //重试间隔的毫秒数
          "RetryIntervalMilliseconds": 100,
          "TimeOutMilliseconds": 10000
          //启用JWT认证
          //"UseJWTAuth": false
          //固定Key认证
          //"AuthKey": "abc"
        },
        {
          "ServiceName": "service2",
          //是否启用熔断
          "IsEnableBreaker": false,
          //熔断前出现允许错误几次
          "ExceptionsAllowedBeforeBreaking": 0,
          //熔断多长时间（毫秒）
          "MillisecondsOfBreak": 10000,
          //最多重试几次，如果为0则不重试
          "MaxRetryTimes": 0,
          //重试间隔的毫秒数
          "RetryIntervalMilliseconds": 100,
          "TimeOutMilliseconds": 0,
          "AuthKey": "abc"
        }
      ],
      // 全局策略
      "Global": {
        //是否启用熔断
        "IsEnableBreaker": false,
        //熔断前出现允许错误几次
        "ExceptionsAllowedBeforeBreaking": 3,
        //熔断多长时间（毫秒）
        "MillisecondsOfBreak": 1000,
        //最多重试几次，如果为0则不重试
        "MaxRetryTimes": 0,
        //重试间隔的毫秒数
        "RetryIntervalMilliseconds": 100,
        "TimeOutMilliseconds": 10000
        //"AuthKey": "cck"
      }
    },
    // 请求日志
    "Log": {
      // 是否启用网关日志记录
      "IsEnabled": true,
      // 是否启用事件总线(RabbitMQ)存储日志，生产环境推荐启用，可以提升程序的性能
      "UseEventBus": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
